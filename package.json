  },
  "scripts": {
    "build": "rm -fr build && NODE_ENV=production webpack",
    "coverage": "jest --coverage --config jest.json",
    "lint": "gulp lint",
    "precommit": "NODE_ENV=pre_commit npm run types && npm run lint",
    "prepush": "npm run lint && npm run types && npm test",
    "createTypesCss": "node helpers/createStylFlow.js",
    "start": "NODE_ENV=development webpack-dev-server",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "test": "jest --no-cache --config jest.json",
    "types": "npm run createTypesCss && flow src",
    "upgrade": "yarn upgrade-interactive --exact --latest"
  },
  "devDependencies": {
