{
  "name": "Minesweeper",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov",
  "engines": {
    "node": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/minesweeper-react.git"
  },
  "scripts": {
    "build": "rm -fr build && NODE_ENV=production webpack",
    "build:gh": "rm -fr build && NODE_ENV=production ROOT_URL=minesweeper-react webpack",
    "coverage": "NODE_ENV=testing jest --coverage --config jest.json",
    "lint": "node lint.js",
    "precommit": "NODE_ENV=pre_commit npm run types && npm run lint",
    "prepush": "npm run lint && npm run types && npm test",
    "createTypesCss": "node helpers/createStylFlow.js",
    "start": "NODE_ENV=development webpack-dev-server",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "test": "NODE_ENV=testing jest --no-cache --config jest.json",
    "types": "npm run createTypesCss && flow",
    "upgrade": "yarn upgrade-interactive --ignore-engines --exact --latest",
    "update-gh": "./helpers/update-gh.sh"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-flow": "7.8.3",
    "@babel/plugin-transform-flow-strip-types": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@babel/plugin-transform-react-jsx": "7.8.3",
    "autoprefixer": "9.7.4",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-flow-react-proptypes": "25.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "bem-css-modules": "1.4.2",
    "css-loader": "3.4.2",
    "css-modules-flow-types-loader": "1.3.0",
    "css-modules-flow-types-printer": "1.3.0",
    "csso": "4.0.2",
    "csso-loader": "0.3.1",
    "csso-webpack-plugin": "2.0.0-beta.1",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.0",
    "file-loader": "5.0.2",
    "flow-bin": "0.116.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "husky": "4.0.10",
    "jest-cli": "24.9.0",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "postcss": "7.0.26",
    "postcss-loader": "3.0.0",
    "preload-webpack-plugin": "2.3.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.12.0",
    "redux": "4.0.5",
    "redux-batched-actions": "0.4.1",
    "redux-devtools-extension": "2.13.8",
    "script-ext-html-webpack-plugin": "2.1.4",
    "style-loader": "1.1.3",
    "stylint": "2.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "svg-url-loader": "3.0.3",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "uglify-es-webpack-plugin": "0.10.0",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-sources": "1.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
