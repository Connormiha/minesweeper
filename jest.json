{
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "styl",
    "css",
    "flow"
  ],
  "transform": {
    ".+\\.(css|styl)$": "<rootDir>/__tests__/preprocessor-stylus.js"
  },
  "testRegex": "/__tests__/.*\\.spec\\.jsx?$",
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "bail": true,
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}"
  ],
  "setupFiles": [
    "<rootDir>/__tests__/setup.js"
  ]
}
