{
  "name": "Minesweeper",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov",
  "engines": {
    "node": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/minesweeper.git"
  },
  "scripts": {
    "build": "rm -fr build && NODE_ENV=production webpack",
    "build:gh": "rm -fr build && NODE_ENV=production ROOT_URL=minesweeper webpack",
    "coverage": "NODE_ENV=testing jest --coverage --config jest.json",
    "lint": "node lint.js",
    "precommit": "NODE_ENV=pre_commit npm run types && npm run lint",
    "prepush": "npm run lint && npm run types && npm test",
    "createTypesCss": "node helpers/createStylFlow.js",
    "start": "NODE_ENV=development webpack-dev-server",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "test": "NODE_ENV=testing jest --no-cache --config jest.json",
    "types": "npm run createTypesCss && flow check",
    "upgrade": "yarn upgrade-interactive --ignore-engines --exact --latest",
    "update-gh": "./helpers/update-gh.sh"
  },
  "devDependencies": {
    "autoprefixer": "9.1.1",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-plugin-flow-react-proptypes": "24.1.1",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "bem-css-modules": "1.2.2",
    "css-loader": "1.0.0",
    "css-modules-flow-types-loader": "1.1.0",
    "css-modules-flow-types-printer": "1.0.0",
    "csso": "3.5.1",
    "csso-loader": "0.3.1",
    "csso-webpack-plugin": "1.0.0-beta.12",
    "enzyme": "3.4.2",
    "eslint": "5.3.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-jest": "21.21.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "1.1.11",
    "flow-bin": "0.78.0",
    "html-webpack-plugin": "3.2.0",
