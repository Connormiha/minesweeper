{
  "name": "Minesweeper",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Mikhail Bodrov",
  "engines": {
    "node": ">=8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Connormiha/minesweeper.git"
  },
  "scripts": {
    "build": "rm -fr build && NODE_ENV=production webpack",
    "build:gh": "rm -fr build && NODE_ENV=production ROOT_URL=minesweeper webpack",
    "coverage": "NODE_ENV=testing jest --coverage --config jest.json",
    "lint": "node lint.js",
    "precommit": "NODE_ENV=pre_commit npm run types && npm run lint",
    "prepush": "npm run lint && npm run types && npm test",
    "createTypesCss": "node helpers/createStylFlow.js",
    "start": "NODE_ENV=development webpack-dev-server",
    "build:open": "http-server build --gzip -p 8000 -s -c-1",
    "test": "NODE_ENV=testing jest --no-cache --config jest.json",
    "types": "npm run createTypesCss && flow",
    "upgrade": "yarn upgrade-interactive --ignore-engines --exact --latest",
    "update-gh": "./helpers/update-gh.sh"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-flow": "7.0.0",
    "@babel/plugin-transform-flow-strip-types": "7.1.6",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-jsx": "7.1.6",
    "autoprefixer": "9.3.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-flow-react-proptypes": "24.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "bem-css-modules": "1.4.1",
    "css-loader": "1.0.1",
    "css-modules-flow-types-loader": "1.1.1",
    "css-modules-flow-types-printer": "1.1.1",
    "csso": "3.5.1",
    "csso-loader": "0.3.1",
    "csso-webpack-plugin": "1.0.0-beta.12",
    "enzyme": "3.7.0",
    "eslint": "5.9.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "flow-bin": "0.86.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "husky": "1.1.4",
    "jest-cli": "23.6.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.4.4",
    "postcss": "7.0.6",
    "postcss-loader": "3.0.0",
    "preload-webpack-plugin": "2.3.0",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.6.3",
    "redux": "4.0.1",
    "redux-batched-actions": "0.4.1",
    "redux-devtools-extension": "2.13.5",
    "script-ext-html-webpack-plugin": "2.1.3",
    "style-loader": "0.23.1",
    "stylint": "^1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "svg-url-loader": "2.3.2",
    "svgo": "1.1.1",
