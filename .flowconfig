[ignore]
.*/node_modules/babel*
.*/node_modules/postcss*
.*/node_modules/webpack*
.*/node_modules/gulp*
.*/node_modules/svgo*
.*/node_modules/csso*
.*/node_modules/stylus*
.*/node_modules/eslint*

[options]
emoji=true
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.system.node.resolve_dirname=__tests__
munge_underscores=true

module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.styl

[lints]
all=warn
