export type CellType = {|
    isOpened: boolean,
    isBomb: boolean,
    isDead: boolean,
    isMarked: boolean,
    id: number,
|};

export type FieldType = CellType[][];

export type GameState = 'lost' | 'win' | 'in-progress' | 'not-started';

export type GameType = {|
    width: number,
    height: number,
    minesCount: number,
    state: GameState,
|};
